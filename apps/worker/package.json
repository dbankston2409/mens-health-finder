{
  "name": "mens-health-finder-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "node fix-imports-correct.js",
    "build": "tsc",
    "postbuild": "mkdir -p dist/utils && cp utils/*.js dist/utils/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "dev": "ts-node index.ts",
    "worker:import": "ts-node cli.ts import",
    "worker:help": "ts-node cli.ts help",
    "deploy:render": "DEPLOY_PLATFORM=render ./deploy.sh",
    "deploy:railway": "DEPLOY_PLATFORM=railway ./deploy.sh",
    "deploy:docker": "DEPLOY_PLATFORM=docker ./deploy.sh"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.5.0",
    "firebase-admin": "^13.3.0",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.4",
    "glob": "^10.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
